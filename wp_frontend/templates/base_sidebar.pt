<html metal:define-macro="base"
      metal:extend-macro="base.macros['base']">
  <tal:block metal:fill-slot="content">
    <aside>
      <tal:block metal:define-slot="sidebar"></tal:block>
    </aside>
    <section class="main">
      <tal:block metal:use-macro="blocks.macros['block-wo-header']">
	<tal:block metal:fill-slot="inner">
	  <tal:block metal:define-slot="main"></tal:block>
	</tal:block>
      </tal:block>
    </section>
  </tal:block>
</html>
